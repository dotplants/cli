{
  "name": "@dotplants/cli",
  "version": "0.1.1",
  "author": ".Plants <oss@dotplants.net>",
  "maintainers": [
    "nzws <nzws@hq.dotplants.net>"
  ],
  "main": "./build/index.js",
  "license": "MIT",
  "repository": "https://github.com/dotplants/cli.git",
  "homepage": "https://github.com/dotplants/cli",
  "engines": {
    "node": "10.x || 12.x || 13.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint:prettier": "pretty-quick --check",
    "lint:eslint": "eslint --ignore-path .gitignore \"**/*.ts\"",
    "lint:tsc": "tsc",
    "lint": "npm-run-all lint:* -s",
    "format:prettier": "pretty-quick",
    "format:js": "eslint --fix --ignore-path .gitignore \"**/*.ts\"",
    "format": "npm-run-all format:* -s",
    "build:ts": "babel src -d build --extensions \".ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm-run-all build:* -s",
    "start:multi": "yarn build:ts && node example/multi.js",
    "start:single": "yarn build:ts && node example/single.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "@yuzulabo/eslint-config": "^1.0.1",
    "@yuzulabo/prettier-config": "~1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "~6",
    "npm-run-all": "~4",
    "prettier": "~1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "package.json",
    "yarn.lock",
    "LICENSE",
    "README.md",
    "/build"
  ],
  "prettier": "@yuzulabo/prettier-config",
  "dependencies": {
    "kleur": "^3.0.3"
  }
}
